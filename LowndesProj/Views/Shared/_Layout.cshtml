@{
    var name = ViewData["UserName"];
    var empID = ViewData["empID"];
    var first = ViewData["nominator_first"];
    var middle = ViewData["nominator_middle"];
    var last = ViewData["nominator_last"];
    var full = ViewData["nominator_full_name"];
    var winID = ViewData["WindowsID"];
}


<html xmlns:ng="http://angularjs.org">

<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Lowndes Employee Nomination">

    <title>Employee Nominations</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/angular-motion.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-additions.css" rel="stylesheet" />
    <link href="~/Content/angular-loading-bar.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />


    @Scripts.Render( "~/bundles/modernizr" )
    <meta name="description" content="The description of my page" />
    <script type="text/javascript">
        window.empid = "@empID";
        window.full_name = "@name";
        window.username = "@name";
        window.VirtualPath = "@HttpRuntime.AppDomainAppVirtualPath";
    </script>
</head>
<body ng-app="nominateApp">
